{
  "helpMessage": "\nPlease fix your commit message (and consider using http://npm.im/commitizen)\n",
  "types": [
    "feat",
    "fix",
    "docs",
    "style",
    "refactor",
    "perf",
    "test",
    "chore",
    "revert",
    "custom"
  ],
  "subjectPattern": "\\bapp.asana.com\\b",
  "subjectPatternErrorMsg": "subject does not match subject pattern!",
  "maxSubjectLength": 500,
  "warnOnFail": false,
  "autoFix": false
}
